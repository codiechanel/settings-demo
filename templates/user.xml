<templateSet group="user">
  <template name="log" value="console.log(&quot;msg: &quot;, item)" description="my logger" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="styled" value="const $name$ = styled.div`&#10;  display: flex;&#10;  background-color: #ff5b5c;&#10;`&#10;&#10;$END$" description="styled div" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="styled2" value="const $SELECTION$ = styled.div`&#10;  display: flex;&#10;  background-color: #ff5b5c;&#10;`&#10;&#10;$END$" description="styled div 2" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="spring1" value="function $name$$SELECTION$() {&#10;  const props = useSpring({opacity: 1, from: {opacity: 0}})&#10;  return &lt;animated.div style={props}&gt;I will fade in&lt;/animated.div&gt;&#10;}&#10;&#10;$END$" description="spring basic" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="spring2" value="const $name$$SELECTION$ = () =&gt; (&#10;  &lt;Spring&#10;    from={{ opacity: 0 }}&#10;    to={{ opacity: 1 }}&#10;    config={{ tension: 30, friction: 10 }}&#10;  &gt;&#10;    {(styles) =&gt; &lt;div style={styles}&gt;I change opacity when I mount&lt;/div&gt;}&#10;  &lt;/Spring&gt;&#10;)&#10;&#10;$END$" description="spring render props" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="useEffect" value="useEffect(() =&gt; {&#10;    $END$&#10;  })" description="useEffect" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="impstyled" value="import styled from &quot;styled-components&quot;;" description="import syled" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="runInAction" value="runInAction(() =&gt; {&#10;          &#10;      })" description="runInAction mobx" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
      <option name="TS_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>